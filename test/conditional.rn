# conditional statements if else

maketh portion average;     # float
maketh verdict passed;      # boolean

# get value for average
heareth("Input average: %d", average); 

if (average < 0 || average > 100) {
    returneth 1;            # exit failure
}

if (average > 75) {
    passed = yay;
    sayeth("Passed!");
} else {
    passed = nay;
    sayeth("Failed");
}
